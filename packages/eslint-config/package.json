{
  "name": "@dg-scripts/eslint-config",
  "type": "module",
  "version": "6.0.0",
  "description": "ESLint configuration used by dg-scripts.",
  "author": "sabertazimi <sabertazimi@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/sabertazimi/bod",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/bod.git",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/bod/issues"
  },
  "keywords": [
    "dg-scripts",
    "lint",
    "linter",
    "prettier",
    "standard",
    "eslint",
    "eslint-prettier"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "index.d.ts",
    "index.js",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "lint": "eslint . --config=index.js",
    "lint:fix": "eslint . --config=index.js --fix",
    "lint:report": "eslint-config-inspector --config=index.js --outDir=dist build"
  },
  "peerDependencies": {
    "@next/eslint-plugin-next": "^16.0.0",
    "eslint": "^9.0.0",
    "next": "^16.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "@next/eslint-plugin-next": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-config": "^6.4.2",
    "@eslint-react/eslint-plugin": "^2.3.12",
    "eslint-plugin-format": "^1.1.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-testing-library": "^7.13.5",
    "local-pkg": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.4.2"
  }
}
