{
  "name": "bod",
  "version": "5.14.0",
  "description": "Boilerplate CLI App",
  "author": "sabertazimi <sabertazimi@gmail.com>",
  "license": "MIT",
  "homepage": "https://sabertazimi.github.io/bod",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/bod.git",
    "directory": "packages/bod"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/bod/issues"
  },
  "keywords": [
    "bod",
    "bod-cli",
    "boilerplate",
    "cli",
    "create-react-app",
    "react",
    "react-scripts",
    "starter",
    "starter-kit",
    "template",
    "typescript"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": "bin/bod.js",
  "files": [
    "bin/bod.js",
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "!dist/**/*.test.js",
    "!dist/**/*.test.d.ts"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf ./dist/",
    "compile": "tsc",
    "dev": "ts-node ./src/bod.ts",
    "format": "prettier --write src/**/*.ts",
    "lint": "yarn lint:style && yarn lint:type-check",
    "lint:style": "eslint --ext .ts ./src",
    "lint:type-check": "tsc --noEmit",
    "start": "yarn dev"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "consola": "^3.2.3",
    "cross-spawn": "^7.0.3",
    "envinfo": "^7.11.0",
    "inquirer": "^8.2.6",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.6",
    "@types/envinfo": "^7.8.3",
    "@types/inquirer": "^8.2.10",
    "@types/rimraf": "^3.0.2",
    "ci-info": "^3.9.0",
    "rimraf": "^5.0.5"
  }
}
