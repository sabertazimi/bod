{
  "name": "bod",
  "version": "5.3.2",
  "description": "Boilerplate CLI App",
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rimraf ./dist/",
    "compile": "tsc",
    "dev": "ts-node ./src/bod.ts",
    "format": "prettier --write src/**/*.ts",
    "lint": "npm run lint:style && npm run lint:type-check",
    "lint:style": "eslint --fix --ext .ts ./src",
    "lint:type-check": "tsc --noEmit",
    "start": "npm run dev",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "!dist/**/*.test.js",
    "!dist/**/*.test.d.ts"
  ],
  "bin": {
    "bod": "bin/bod.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/bod.git",
    "directory": "packages/bod"
  },
  "keywords": [
    "bod",
    "bod-cli",
    "boilerplate",
    "cli",
    "create-react-app",
    "react",
    "react-scripts",
    "starter",
    "starter-kit",
    "template",
    "typescript"
  ],
  "author": "sabertazimi <sabertazimi@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/bod/issues"
  },
  "homepage": "https://sabertazimi.github.io/bod",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^8.1.0",
    "consola": "^2.15.3",
    "cross-spawn": "^7.0.3",
    "envinfo": "^7.8.1",
    "inquirer": "^8.1.2"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.2",
    "@types/envinfo": "^7.8.1",
    "@types/inquirer": "^7.3.3",
    "@types/rimraf": "^3.0.2",
    "ci-info": "^3.2.0",
    "rimraf": "^3.0.2"
  }
}
