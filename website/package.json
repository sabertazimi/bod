{
  "name": "bod-website",
  "private": true,
  "scripts": {
    "build:docs": "docusaurus build && yarn build:post",
    "build:post": "cp -fr build ../",
    "clear": "docusaurus clear",
    "lint": "yarn lint:style && yarn lint:type-check",
    "lint:style": "eslint --ext .ts,.tsx ./src && stylelint ./src/**/*.css",
    "lint:type-check": "tsc --noEmit",
    "serve": "docusaurus serve",
    "start": "docusaurus start"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.8",
    "@docusaurus/preset-classic": "2.0.0-beta.8",
    "@easyops-cn/docusaurus-search-local": "^0.19.1",
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.0.0-beta.8",
    "@tsconfig/docusaurus": "^1.0.4",
    "@types/react": "^17.0.33",
    "@types/react-helmet": "^6.1.4",
    "@types/react-router-dom": "^5.3.2",
    "nodejieba": "^2.5.2",
    "prism-react-renderer": "^1.2.1",
    "typescript": "^4.4.4"
  }
}
